{
  "name": "bstudios-telemetry-analytics",
  "version": "0.1.0",
  "private": false,
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "deploy": "wrangler pages deploy ./build/client",
    "dev": "remix vite:dev",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "wrangler pages dev ./build/client",
    "typecheck": "tsc",
    "pages:deploy": "npm run build && wrangler pages deploy ./public",
    "migrations:gen": "drizzle-kit generate:sqlite",
    "migrations:upgradeMetadata": "drizzle-kit up:sqlite",
    "migrations:check": "drizzle-kit check:sqlite",
    "d1:local:migrations:apply": "wrangler d1 migrations apply DB --local",
    "d1:local:init": "npm run d1:local:migrations:apply && wrangler d1 execute DB --file='./app/db/fixtures/demo.sql' --local",
    "d1:local:reset": "wrangler d1 execute DB --local --file='./app/db/fixtures/reset.sql' && npm run d1:local:init",
    "typegen": "wrangler types"
  },
  "dependencies": {
    "@mantine/charts": "^7.8.0",
    "@mantine/code-highlight": "^7.8.0",
    "@mantine/core": "^7.8.0",
    "@mantine/dates": "^7.8.0",
    "@mantine/form": "^7.8.0",
    "@mantine/hooks": "^7.8.0",
    "@mantine/modals": "^7.8.0",
    "@mantine/notifications": "^7.8.0",
    "@mantine/nprogress": "^7.8.0",
    "@remix-run/cloudflare": "^2.8.1",
    "@remix-run/cloudflare-pages": "^2.8.1",
    "@remix-run/css-bundle": "^2.4.0",
    "@remix-run/react": "^2.8.1",
    "@remix-validated-form/with-zod": "^2.0.7",
    "@tabler/icons-react": "^3.2.0",
    "@typescript-eslint/parser": "^6.16.0",
    "dayjs": "^1.11.10",
    "drizzle-orm": "^0.29.1",
    "eslint-plugin-drizzle": "^0.2.3",
    "isbot": "^4.1.0",
    "miniflare": "^3.20231030.4",
    "nanoid": "^5.0.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^2.12.5",
    "remix-validated-form": "^5.1.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230518.0",
    "@remix-run/dev": "^2.8.1",
    "@remix-run/eslint-config": "^2.4.1",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "drizzle-kit": "^0.20.7",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "node-fetch": "^3.3.2",
    "postcss": "^8.4.38",
    "postcss-preset-mantine": "^1.14.4",
    "postcss-simple-vars": "^7.0.1",
    "typescript": "^5.1.6",
    "vite": "^5.1.0",
    "vite-tsconfig-paths": "^4.2.1",
    "wrangler": "^3.24.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
